<h1> For your New Posts</h1>

<p><%= form_for :post, url: posts_path do |f| %>
    <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :body %><br>
      <%= f.text_field :description %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end %>

</p>
